    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: main",
            "type": "debugpy",
            "args": ["--training", "algo_type=2"],
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
      "name": "Streamlit",
      "type": "debugpy",
      "request": "launch",
      "program": "venv\\Scripts\\streamlit.exe",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["run", "src\\app.py"]
    },
    ]
}